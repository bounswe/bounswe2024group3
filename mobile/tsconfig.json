{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@env": [
        "./env.d.ts"
      ]
    },
    "typeRoots": [
      "./node_modules/@types",
      "./"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "context/ThemeContext.js",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
